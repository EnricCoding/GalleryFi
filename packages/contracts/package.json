{
  "name": "contracts",
  "version": "1.0.0",
  "description": "This project demonstrates a basic Hardhat use case. It comes with a sample contract, a test for that contract, and a Hardhat Ignition module that deploys that contract.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf cache artifacts typechain typechain-types",
    "compile": "hardhat compile",
    "typechain": "hardhat typechain",
    "build": "npm run compile && npm run typechain",
    "test": "npx hardhat test",
    "coverage": "hardhat coverage",
    "gas": "REPORT_GAS=true hardhat test",
    "deploy:sepolia": "hardhat deploy --network sepolia --tags all",
    "verify:sepolia": "hardhat verify --network sepolia"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@nomiclabs/hardhat-solhint": "^4.1.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "dotenv": "^17.2.1",
    "hardhat": "^2.26.1",
    "hardhat-deploy": "^1.0.4",
    "hardhat-gas-reporter": "^2.3.0",
    "solhint": "^6.0.0",
    "solidity-coverage": "^0.8.16",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.8.3"
  }
}
